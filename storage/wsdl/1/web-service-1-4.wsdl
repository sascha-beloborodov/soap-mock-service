                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="Pretension">
                <xsd:sequence>
                    <xsd:element name="order_number" type="xsd:string" />
                    <xsd:element name="pretension" type="xsd:string" />
                </xsd:sequence>
            </xsd:complexType>

            <!-- Pretension status -->
            <xsd:element name="getPretensionStatusObject">
