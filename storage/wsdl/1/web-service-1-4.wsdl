                        <xsd:element name="payment_date" type="xsd:string" />
                        <xsd:element name="request_code" type="xsd:string" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="BankTransferOrder">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string" />
                </xsd:sequence>
