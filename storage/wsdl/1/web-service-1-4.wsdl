                        <xsd:element name="status" type="xsd:string" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <!-- Command status request -->
            <xsd:element name="getRequestStatusObject">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="request_code" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getRequestStatusResponseObject">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="response" type="tns:RequestStatusResponse" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="RequestStatusResponse">
                <xsd:sequence>
                    <!-- Variable response -->
                    <xsd:element name="contraparty" type="tns:Contraparty" minOccurs="0" nillable="true"/>
                    <xsd:element name="order_plan_status" type="tns:OrderPlanStatus" minOccurs="0" nillable="true"/>
                    <xsd:element name="planning_department_answer" type="tns:PlanningDepartmentAnswer" minOccurs="0" nillable="true"/>
                    <xsd:element name="order_delivery_allowance" type="tns:OrderDeliveryAllowance" minOccurs="0" nillable="true" />
                    <xsd:element name="bank_transfer_order" type="tns:BankTransferOrder" minOccurs="0" nillable="true" />
                    <xsd:element name="order_cancellation" type="tns:OrderCancellation" minOccurs="0" nillable="true" />
                    <xsd:element name="order_delivery_disallowance" type="tns:OrderDeliveryDisallowance" minOccurs="0" nillable="true" />
                    <xsd:element name="outflow_status" type="tns:OutflowStatus" minOccurs="0" nillable="true" />
                    <xsd:element name="pretension" type="tns:Pretension" minOccurs="0" nillable="true" />
                    <xsd:element name="operation_fault" type="tns:OperationFault" minOccurs="0" nillable="true" />
                </xsd:sequence>
                <xsd:attribute name="state" type="tns:StatusResponseState" use="required"/>
            </xsd:complexType>
            <xsd:simpleType name="StatusResponseState">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="not_received"/>
                    <xsd:enumeration value="error"/>
                    <xsd:enumeration value="success"/>
                    <xsd:enumeration value="expired"/>
                </xsd:restriction>
            </xsd:simpleType>

            <xsd:element name="submittedRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="request_code" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="OperationFault">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string" />
                    <xsd:element name="description" type="xsd:string" />
                </xsd:sequence>
            </xsd:complexType>
        </xsd:schema>
    </wsdl:types>

