<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="urn:tnwebservice" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap-enc="http://schemas.xmlsoap.org/soap/encoding/" name="TNWebService" targetNamespace="urn:tnwebservice">
    <wsdl:documentation>
        Service: TN 1C Web Service for E-Commerce
        Version: 1.2 (describes Integration Specification v1.2)
    </wsdl:documentation>
    <wsdl:types>
        <xsd:schema targetNamespace="urn:tnwebservice" elementFormDefault="qualified" attributeFormDefault="qualified">
            <xsd:complexType name="ArrayOfString">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ArrayOfUri">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="xsd:anyURI"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Service auth element -->
            <xsd:element name="serviceAuth">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="key" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <!-- Products information -->
            <xsd:element name="getProductsList">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductsListResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductEntity"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductEntity">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductEntity"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ProductEntity">
                <xsd:sequence>
                    <xsd:element name="group" type="xsd:string" />
                    <xsd:element name="code" type="xsd:string" />
                    <xsd:element name="ekn" type="xsd:string" />
                    <xsd:element name="name" type="xsd:string" />
                    <xsd:element name="doc_name" type="xsd:string" />
                    <xsd:element name="spec_unit" type="xsd:string" />
                    <xsd:element name="base_unit" type="xsd:string" />
                    <xsd:element name="shipping_place_unit" type="xsd:string" />
                    <xsd:element name="shipping_place_unit_adjustment" type="tns:ArrayOfProductShippingPlaceUnitAdjustment" />
                    <xsd:element name="measure_units" type="tns:ArrayOfProductMeasureUnit" />
                    <xsd:element name="description" type="xsd:string" minOccurs="0" nillable="true"/>
                    <xsd:element name="application" type="xsd:string" minOccurs="0" nillable="true"/>
                    <xsd:element name="storing" type="xsd:string" minOccurs="0" nillable="true"/>
                    <xsd:element name="attributes" type="tns:ArrayOfProductAttributeValue" />
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ArrayOfProductShippingPlaceUnitAdjustment">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductShippingPlaceUnitAdjustment"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductShippingPlaceUnitAdjustment">
                <xsd:sequence>
                    <xsd:element name="city" type="xsd:string"/>
                    <xsd:element name="code" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ArrayOfProductMeasureUnit">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductMeasureUnit"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductMeasureUnit">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="classifier_code" type="xsd:string"/>
                    <xsd:element name="volume" type="xsd:double"/>
                    <xsd:element name="net_weight" type="xsd:double"/>
                    <xsd:element name="gross_weight" type="xsd:double"/>
                    <xsd:element name="weight_adjustment" type="tns:ArrayOfProductWeightAdjustment"/>
                    <xsd:element name="coeff" type="xsd:decimal"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ArrayOfProductWeightAdjustment">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductWeightAdjustment"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductWeightAdjustment">
                <xsd:sequence>
                    <xsd:element name="city" type="xsd:string"/>
                    <xsd:element name="gross_weight" type="xsd:double"/>
                    <xsd:element name="net_weight" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ArrayOfProductAttributeValue">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductAttributeValue"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductAttributeValue">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="value" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Products groups information -->
            <xsd:element name="getProductsGroups">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="groups" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductsGroupsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductGroup"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductGroup">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductGroup"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductGroup">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="name" type="xsd:string"/>
                    <xsd:element name="attributes" type="tns:ArrayOfString"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Products attributes information -->
            <xsd:element name="getProductsAttributes">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="attributes" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductsAttributesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductAttribute"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductAttribute">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductAttribute"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductAttribute">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="name" type="xsd:string"/>
                    <xsd:element name="type" type="tns:ProductAttributeType"/>
                    <xsd:element name="options" type="tns:ArrayOfProductAttributeOption" minOccurs="0" nillable="true"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ArrayOfProductAttributeOption">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductAttributeOption"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductAttributeOption">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="label" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="ProductAttributeType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="varchar"/>
                    <xsd:enumeration value="text"/>
                    <xsd:enumeration value="options"/>
                    <xsd:enumeration value="decimal"/>
                </xsd:restriction>
            </xsd:simpleType>

            <!-- Products prices information -->
            <xsd:element name="getProductsPrices">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                        <xsd:element name="cities" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getChangedProductsPrices">
                <xsd:complexType>
                    <xsd:sequence />
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductsPricesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductPrice"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductPrice">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductPrice"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductPrice">
                <xsd:sequence>
                    <xsd:element name="product" type="xsd:string"/>
                    <xsd:element name="price" type="xsd:decimal"/>
                    <xsd:element name="measure_unit" type="xsd:string"/>
                    <xsd:element name="city" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Products stock information -->
            <xsd:element name="getProductsStock">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                        <xsd:element name="cities" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getChangedProductsStock">
                <xsd:complexType>
                    <xsd:sequence />
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductsStockResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductStock"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductStock">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductStock"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductStock">
                <xsd:sequence>
                    <xsd:element name="product" type="xsd:string"/>
                    <xsd:element name="qty" type="xsd:double"/>
                    <xsd:element name="measure_unit" type="xsd:string"/>
                    <xsd:element name="warehouse" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Loading rates information -->
            <xsd:element name="getLoadingRates">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                        <xsd:element name="cities" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getLoadingRatesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfLoadingRate"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfLoadingRate">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:LoadingRate"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="LoadingRate">
                <xsd:sequence>
                    <xsd:element name="product" type="xsd:string"/>
                    <xsd:element name="transport" type="xsd:string"/>
                    <xsd:element name="qty" type="xsd:double"/>
                    <xsd:element name="measure_unit" type="xsd:string"/>
                    <xsd:element name="city" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- QTY restrictions rates information -->
            <xsd:element name="getQtyRestrictions">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                        <xsd:element name="warehouses" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getQtyRestrictionsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfQtyRestriction"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfQtyRestriction">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:QtyRestriction"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="QtyRestriction">
                <xsd:sequence>
                    <xsd:element name="product" type="xsd:string"/>
                    <xsd:element name="qty" type="xsd:double"/>
                    <xsd:element name="measure_unit" type="xsd:string"/>
                    <xsd:element name="rule" type="tns:QtyRestrictionRule"/>
                    <xsd:element name="warehouse" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="QtyRestrictionRule">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="min_qty"/>
                    <xsd:enumeration value="fold"/>
                </xsd:restriction>
            </xsd:simpleType>

            <!-- Transports information -->
            <xsd:element name="getTransports">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="transports" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getTransportsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfTransport"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfTransport">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:Transport"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Transport">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="name" type="xsd:string"/>
                    <xsd:element name="weight_capacity" type="xsd:double"/>
                    <xsd:element name="cubic_capacity" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Transport loading methods information -->
            <xsd:element name="getTransportLoadingMethods">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="methods" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getTransportLoadingMethodsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfTransportLoadingMethod"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfTransportLoadingMethod">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:TransportLoadingMethod"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="TransportLoadingMethod">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="name" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Cities information -->
            <xsd:element name="getCities">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="cities" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getCitiesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfCity"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfCity">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:City"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="City">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="name" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Product scheduling techniques information -->
            <xsd:element name="getProductSchedulingTechniques">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="products" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                        <xsd:element name="warehouses" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getProductSchedulingTechniquesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfProductSchedulingTechnique"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfProductSchedulingTechnique">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:ProductSchedulingTechnique"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ProductSchedulingTechnique">
                <xsd:sequence>
                    <xsd:element name="product" type="xsd:string"/>
                    <xsd:element name="warehouse" type="xsd:string"/>
                    <xsd:element name="value" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Scheduling techniques information -->
            <xsd:element name="getSchedulingTechniques">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="techniques" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getSchedulingTechniquesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfSchedulingTechnique"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfSchedulingTechnique">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:SchedulingTechnique"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SchedulingTechnique">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="label" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Measure units information -->
            <xsd:element name="getMeasureUnits">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="units" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getMeasureUnitsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfMeasureUnit"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfMeasureUnit">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:MeasureUnit"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="MeasureUnit">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="label" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Pretension statuses information -->
            <xsd:element name="getPretensionStatuses">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="statuses" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getPretensionStatusesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfPretensionStatus"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfPretensionStatus">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:PretensionStatus"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="PretensionStatus">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="label" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Warehouses information -->
            <xsd:element name="getWarehouses">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="warehouses" type="tns:ArrayOfString" nillable="true" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="getWarehousesResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="tns:ArrayOfWarehouse"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="ArrayOfWarehouse">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:Warehouse"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Warehouse">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                    <xsd:element name="label" type="xsd:string"/>
                    <xsd:element name="city" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Submit contraparty -->
            <xsd:element name="submitContrapartyObject">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" />
                        <xsd:element name="inn" type="xsd:string" />
                        <xsd:element name="kpp" type="xsd:string" />
                        <xsd:element name="okpo" type="xsd:string" />
                        <xsd:element name="payment_account" type="xsd:string" />
                        <xsd:element name="bik" type="xsd:string" />
                        <xsd:element name="currency" type="xsd:string" />
                        <xsd:element name="legal_address" type="xsd:string" />
                        <xsd:element name="request_code" type="xsd:string" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="Contraparty">
                <xsd:sequence>
                    <xsd:element name="code" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <!-- Submit order -->
            <xsd:element name="submitOrderObject">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="order_number" type="xsd:string" />
                        <xsd:element name="order_date" type="xsd:string" />
                        <xsd:element name="contraparty" type="xsd:string" />
                        <xsd:element name="last_name" type="xsd:string" />
                        <xsd:element name="first_name" type="xsd:string" />
                        <xsd:element name="middle_name" type="xsd:string" />
                        <xsd:element name="phone" type="xsd:string" />
                        <xsd:element name="dispatch_date" type="xsd:string" />
                        <xsd:element name="transport" type="xsd:string" />
                        <xsd:element name="addresses" type="tns:ArrayOfOrderAddress" />
                        <xsd:element name="services" type="tns:ArrayOfOrderService" />
                        <xsd:element name="request_code" type="xsd:string" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="ArrayOfOrderAddress">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:OrderAddress"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="OrderAddress">
                <xsd:sequence>
                    <xsd:element name="latitude" type="xsd:decimal"/>
                    <xsd:element name="longitude" type="xsd:decimal"/>
                    <xsd:element name="address" type="xsd:string"/>
                    <xsd:element name="last_name" type="xsd:string"/>
                    <xsd:element name="first_name" type="xsd:string"/>
                    <xsd:element name="middle_name" type="xsd:string"/>
                    <xsd:element name="phone" type="xsd:string"/>
                    <xsd:element name="delivery_date" type="xsd:string"/>
                    <xsd:element name="loading_method" type="xsd:string"/>
                    <xsd:element name="items" type="tns:ArrayOfOrderItem"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ArrayOfOrderItem">
                <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="tns:OrderItem"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="OrderItem">
                <xsd:sequence>
